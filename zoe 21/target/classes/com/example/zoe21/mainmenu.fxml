<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane fx:id="gameLayout" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.zoe21.MainController">

    <children>
        <Button fx:id="highscoreButton" layoutX="248.0" layoutY="155.0" onAction="#onHighScoreButtonClick" text="Highscore" />
        <Button fx:id="regularGameButton" layoutX="238.0" layoutY="200.0" onAction="#onRegularGameButtonClick" text="Regular Game" />
        <Button fx:id="machineGameButton" layoutX="236.0" layoutY="246.0" onAction="#onMachineGameButtonClick" text="Machine Game" />
        <Label fx:id="welcomeText" layoutX="258.0" layoutY="113.0" text="Welcome!" />
    </children>
</AnchorPane>
